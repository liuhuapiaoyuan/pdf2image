# 服务配置
HOST=0.0.0.0
PORT=8000
DEBUG=false

# 文件存储配置
OUTPUT_STORAGE_TYPE=both  # base64, file, both
OUTPUT_DIR=/tmp/pdf2image_output
OUTPUT_BASE_URL=http://localhost:8000/files

# PDF处理配置
MAX_FILE_SIZE=52428800  # 50MB in bytes
DEFAULT_DPI=200
DEFAULT_FORMAT=png
MAX_THREAD_COUNT=8
DEFAULT_TIMEOUT=600

# 安全配置
CORS_ORIGINS=*

# Poppler配置
# POPPLER_PATH=/usr/bin

# 缓存配置
ENABLE_CACHE=false
CACHE_TTL=3600